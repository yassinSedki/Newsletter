
{% extends "template.html.twig" %} 
{% block title%}
         add newsletter
        {% endblock %}
{% block stylesheets %}
  <link rel="stylesheet" href="{{ asset('css/form.css') }}">
{% endblock %}

{% block pageTitle %}
create a newsletter        {% endblock %}
        {% block body %}
     {{ form_start(form, {'attr': {'novalidate': 'novalidate'}})}}
<div class="form-group">
{{ form_row(form.name,{'attr':{'class':'form-control'}}) }}
</div>
<div class="form-group">
{{ form_row(form.content,{'attr':{'class':'form-control'}}) }}
</div>
<div class="form-group">
    {{ form_label(form.category, 'Category', {'label_attr': {'class': 'mr-sm-2'}}) }}
    {{ form_widget(form.category, {'attr': {'class': 'form-select'}}) }}
    {{ form_errors(form.category) }}
</div>

{{form_row(form.send,{'attr':{'class':'btn btn-success'}})}}

{{form_end(form)}}



        {% endblock %}
